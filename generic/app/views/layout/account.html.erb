<%= partial 'layout/main' %>
       <div id="sidebar">
      <ul id="menu"><!-- Nested Lists -->
            <% @access_rights = session.user.access_peoples  %>
         <% if @access_rights.length == 0 %>
             <li <% if @selected == "parent_profile" %> class="on" <% end %>><%= link_to "My Profile", url(:parent_account) %></li>
             <li <% if @selected == "s_details" %> class="on" <% end %>><%= link_to "Student Details", url(:student_details) %></li>
             <li <% if @selected == "other_family" %> class="on" <% end %>><%= link_to "Other Family Members", url(:parent_account, :label => "other") %></li>
             <li <% if @selected == "alerts" %> class="on" <% end %>><%#= link_to "Alert Settings", url(:alerts) %></li>
         <% else %>
              <li <% if @selected == "staff_profile" %> class="on" <% end %>><%= link_to "My Profile", url(:staff_account) %></li>
              <li <% if @selected == "alerts" %> class="on" <% end %>><%#= link_to "Alert Settings", url(:alerts) %></li>
         <% end %>
  
      </ul>

    </div><!-- Sidebar Ends -->
    <div id="content-wrap"><!--Wraps The content Area-->
      <div class="hrule clear hidden "><hr /></div>
      <div id="content"><!--Content Area Starts here-->
        <%= catch_content :for_layout %>
      </div><!--Content End-->
    </div><!--Content Wrap End -->
    
    <div class="hrule clear hidden "><hr /></div>
  </div><!--Pagewrap End -->
  <%= partial 'common/change' %>
</body>
</html>
