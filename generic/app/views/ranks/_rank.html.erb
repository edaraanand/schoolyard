<ul id="ranking">
    <% @r = (1..5).to_a %>
  
  <% @r.each do %>
         <li>
              <%= text_field  :name => "rank[name][]", :class => "short defaultText defaultTextActive", :title => "A +" %>
              <%= text_field  :name => "rank[from][]", :class => "short apart defaultText defaultTextActive number", :title => "90" %> 
              <%= text_field  :name => "rank[to][]", :class => "short apart defaultText defaultTextActive number", :title => "100" %>
              <a href="#"  class="delete-btn"  onclick="this.parentNode.parentNode.removeChild(this.parentNode);"></a>
         </li>
       
  <% end %>

</ul>
