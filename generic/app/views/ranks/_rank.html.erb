<ul id="ranking">
    <% @r = (1..5).to_a %>
  
  <% @r.each do %>
         <li>
              <%= text_field  :name => "rank[name][]", :class => "short" %>
              <%= text_field  :name => "rank[from][]", :class => "short apart" %> 
              <%= text_field  :name => "rank[to][]", :class => "short apart" %>
              <a href="#"  class="delete-btn"  onclick="this.parentNode.parentNode.removeChild(this.parentNode);"></a>
         </li>
       
  <% end %>

</ul>
